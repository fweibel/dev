{
  "name": "arangojs",
  "version": "3.0.0",
  "description": "The official ArangoDB JavaScript driver.",
  "main": "index.js",
  "keywords": [
    "arango",
    "arangodb",
    "aql",
    "nosql",
    "client",
    "driver",
    "api",
    "http",
    "rest"
  ],
  "files": [
    "lib/",
    "index.js",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "npm run lint && npm run test-only",
    "test-only": "mocha -t 30000 --growl -R spec",
    "cover": "npm run lint && istanbul cover --report lcov _mocha -- -t 30000 -R spec",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls ; rm -rf ./coverage",
    "watch": "dirwatch -d 500 -c 'npm run test-only && npm run lint && npm run dist' ./lib ./test",
    "dist": "npm run bundle && npm run minify",
    "bundle": "npm run all-bundle && npm run raw-bundle",
    "minify": "npm run all-minify && npm run raw-minify",
    "all-bundle": "browserify -t dekeywordify -s arangojs ./index.js > dist/arango.all.js",
    "all-minify": "uglifyjs dist/arango.all.js > dist/arango.all.min.js",
    "raw-bundle": "browserify -t dekeywordify -x extend -x request -s arangojs ./index.js > dist/arango.js",
    "raw-minify": "uglifyjs dist/arango.js > dist/arango.min.js",
    "lint": "jshint index.js lib test",
    "prepublish": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arangodb/arangodbjs.git"
  },
  "author": {
    "name": "Alan Plum",
    "email": "me@pluma.io"
  },
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/arangodb/arangodbjs/issues"
  },
  "homepage": "https://github.com/arangodb/arangodbjs",
  "devDependencies": {
    "async": "^0.9.0",
    "browserify": "^7.0.2",
    "coveralls": "^2.11.2",
    "dekeywordify": "^0.2.0",
    "dirwatch": "^1.1.0",
    "expect.js": "^0.3",
    "istanbul": "^0.3.5",
    "jshint": "^2.5.6",
    "mocha": "^2.0.1",
    "uglify-js": "^2.4.15",
    "xhr": "^2.0.0"
  },
  "browser": {
    "request": "xhr"
  },
  "dependencies": {
    "extend": "^2.0.0",
    "request": "^2.42.0"
  },
  "gitHead": "7dde0f91b773eb45234a1b82d36c968aed8c2caf",
  "_id": "arangojs@3.0.0",
  "_shasum": "ef0f668ed3a0e854e034f135cd6fd9f677d6b4a3",
